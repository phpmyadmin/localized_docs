msgstr "Comentário de texto opcional."

#: ../phpmyadmin/doc/import_export.rst:181
msgid "Object name - either table or database based on :js:data:`type`."
msgstr "Nome do objeto - tabela ou banco de dados, com base no :js:data:`type`."

#: ../phpmyadmin/doc/import_export.rst:185
msgid "Database name for ``table`` :js:data:`type`."
msgstr "Nome do banco de dados para o :js:data:`type` ``table``."

#: ../phpmyadmin/doc/import_export.rst:189
msgid "Table content for ``table`` :js:data:`type`."
msgstr "Conteúdo da tabela para o :js:data:`type` ``table``."

