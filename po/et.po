msgstr ""

#: ../phpmyadmin/doc/relations.rst:49
msgid "`category.name`"
msgstr ""

#: ../phpmyadmin/doc/relations.rst:50
msgid "`link.link_id`"
msgstr ""

#: ../phpmyadmin/doc/relations.rst:51
msgid "`link.category_id`"
msgstr ""

#: ../phpmyadmin/doc/relations.rst:52
