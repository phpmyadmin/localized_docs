#: ../phpmyadmin/Documentation.html:2439
#, fuzzy
msgid "<tt>text_html__formatted.inc.php</tt>"
msgstr "text_html__formatted.inc.php"

#. type: Content of: <html><body><div><ol><li>
#: ../phpmyadmin/Documentation.html:2440
#, fuzzy
msgid "<tt>PMA_transform_text_html__formatted()</tt>"
msgstr "PMA_transform_text_html__formatted()"

#. type: Content of: <html><body><div><ol><li>
