#: ../phpMyAdmin/Documentation.html:1675
msgid "iconv - use iconv or libiconv functions"
msgstr ""

#. type: Content of: <html><body><div><dl><dd><ul><li>
#: ../phpMyAdmin/Documentation.html:1676
msgid "recode - use recode_string function"
msgstr ""

