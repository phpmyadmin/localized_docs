msgstr ""

#: ../source/faq.rst:1198
# 2187deca5d23415f808a3b0e372867c2
msgid "Some browsers support right-clicking into the frame you want to refresh, just do this in the right frame."
msgstr ""

#: ../source/faq.rst:1204
# 4a24b760728e40968e16b952ee7a1f61
msgid "5.8 With Mozilla 0.9.7 I have problems sending a query modified in the query box."
msgstr ""

#: ../source/faq.rst:1206
