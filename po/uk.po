#: ../phpmyadmin/docs/config.rst:2915 ../phpmyadmin/docs/config.rst:2928
msgid "``structure``"
msgstr "``structure``"

#: ../phpmyadmin/docs/config.rst:2346 ../phpmyadmin/docs/config.rst:2362
#: ../phpmyadmin/docs/config.rst:2916 ../phpmyadmin/docs/config.rst:2929
msgid "``sql``"
msgstr "``sql``"

#: ../phpmyadmin/docs/config.rst:2347 ../phpmyadmin/docs/config.rst:2363
#: ../phpmyadmin/docs/config.rst:2917 ../phpmyadmin/docs/config.rst:2930
msgid "``search``"
msgstr "``search``"

#: ../phpmyadmin/docs/config.rst:2348 ../phpmyadmin/docs/config.rst:2364
#: ../phpmyadmin/docs/config.rst:2931
