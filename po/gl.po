#: ../phpMyAdmin/Documentation.html:1701
#, no-wrap
msgid ""
"$cfg['TrustedProxies'] =\n"
"     array('1.2.3.4' =&gt; 'HTTP_X_FORWARDED_FOR');\n"
msgstr ""

