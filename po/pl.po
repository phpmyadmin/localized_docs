#, fuzzy
msgid "<tt>PMA_transform_text_html__formatted()</tt>"
msgstr "PMA_transform_text_html__sformatowany()"

#. type: Content of: <html><body><div><ol><li>
#: ../phpmyadmin/Documentation.html:2482
msgid "A mimetype (w/o subtype) transform:"
msgstr "Transformacja typu MIME (bez podtypu):"

