msgstr "このファイルそのものには変換関数は定義しません。"

#. type: Content of: <html><body><div><ol><li>
#: ../phpmyadmin/Documentation.html:2493
#, fuzzy
msgid "<tt>text_plain.inc.php</tt>"
msgstr "text_plain.inc.php"

#. type: Content of: <html><body><div><ol><li>
#: ../phpmyadmin/Documentation.html:2494 ../phpmyadmin/Documentation.html:2509
